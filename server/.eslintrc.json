{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "prettier",
        "airbnb-base"
    ],
    "plugins": [
      "prettier"
    ],
    "parser": "esprima",
    "parserOptions": {
        "ecmaVersion": 6
    },
    "rules": {
      "prettier/prettier": "error",
      "no-console": "off",
      "func-names": "off",
      "implicit-arrow-linebreak": ["error", "below"],
      "function-paren-newline": "off"
    }
}
